centroidS = mean(trainfeatures(:,1:10));
centroidT = mean(trainfeatures(:,11:20));
centroiV  = mean(trainfeatures(:,21:30));

function label = nearcen(point)
distS =  pdist(centroidS,point);
distT =  pdist(centroidT,point);
distV =  pdist(centroidV,point);
mindist = min(distS, distT, distV);
if (min == distS) 
    label = 1;
end

end